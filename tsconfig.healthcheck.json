{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "declaration": false,
    "sourceMap": true,
    "outDir": ".",
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "lib": ["ES2022"],
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true
  },
  "include": [
    "./docker_healthcheck.ts"
  ]
}
